head

  include /views/elements/headscripts
  
  link(href="/resources/elements/cucumbery.css" type="text/css" rel="stylesheet")
  link(href="/resources/elements/elements.css" type="text/css" rel="stylesheet")
  if (elements.includes('scripts')) 
    script(src="https://wany.io/resources/elements/lapis.min.mjs" type="module")

  meta(charset="UTF-8")
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  meta(name="format-detection" content="telephone=no")
  meta(name="theme-color" content="#52EE52")

  if meta.desc
    meta(name="description" content=meta.desc)
  if meta.keyword
    meta(name="keywords" content=meta.keywords)
  if meta.author
    meta(name="author" content=meta.author)

  //- OpenGraph
  meta(name="og:type" content="website")
  meta(name="og:locale" content="ko_KR")
  meta(name="og:site_name" content=meta.og.sitename)
  meta(name="og:title" content=meta.og.title)
  if meta.og.desc
    meta(name="og:description" content=meta.og.desc)
  if meta.og.image
    meta(name="og:image" content=meta.og.image)
    meta(name='og:image:width' content='1200')
    meta(name='og:image:height' content='630')

  //- Twitter
  meta(property='twitter:domain', content='cucumbery.com')
  meta(name='twitter:title', content=meta.og.title)
  if meta.og.desc
    meta(name='twitter:description', content=meta.og.desc)
  if meta.og.image
    meta(name="twitter:image" content=meta.og.image)
    meta(name='twitter:card', content='summary_large_image')

  //- JSON+LD
  if (meta.jsonld)
    script(type="application/ld+json")!= meta.jsonld

  //- Google Analytics
  meta(name="google-site-verification" content="-Dlebff4YUpW_L07L7Yl8FpfB4upSR-H_vOccRw6UgQ")

  title!= title